{
  "name": "clean-tagged-string",
  "version": "0.0.3",
  "description": "A simple utility function to clean ES6 template strings.",
  "main": "index.js",
  "scripts": {
    "build": "babel src/index.js -o ./index.js",
    "test": "ava --tap",
    "dev": "concurrently \"ava --fail-fast --watch\" \"babel src/index.js -o ./index.js --watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taxigy/clean-tagged-string.git"
  },
  "keywords": [
    "es6",
    "es2015",
    "template",
    "strings",
    "tagged",
    "template",
    "strings",
    "node",
    "javascript"
  ],
  "author": "Rishat Muhametshin <rishat@rishat.us> (http://rishat.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taxigy/clean-tagged-string/issues"
  },
  "homepage": "https://github.com/taxigy/clean-tagged-string#readme",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.19.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.7.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^3.4.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
